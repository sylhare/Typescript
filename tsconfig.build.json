{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["WebWorker","es6", "es2019"],
    "sourceMap": true,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
