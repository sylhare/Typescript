{
  "name": "typescript",
  "version": "0.0.0",
  "description": "Experience with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix src tests --ext=.ts",
    "test": "jest",
    "test:root": "jest --selectProjects challenges models service tutorial",
    "test:projects": "jest --selectProjects monorepo-general monorepo-express",
    "test:watch": "jest --verbose --detectOpenHandles",
    "test:cov": "jest --coverage",
    "start": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-sylhare:sylhare/Typescript.git"
  },
  "keywords": [
    "Typescript",
    "fun"
  ],
  "author": "sylhare",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^9.22.0",
    "eslint-plugin-jest": "^29.1.0",
    "jest": "^29.7.0",
    "jest-extended": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
